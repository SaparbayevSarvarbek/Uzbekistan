<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/adminFragment2">

    <fragment
        android:id="@+id/adminFragment2"
        android:name="com.example.uzbekistan.fragmnets.AdminFragment"
        android:label="fragment_admin"
        tools:layout="@layout/fragment_admin" >
        <action
            android:id="@+id/action_adminFragment2_to_adminAddFragment"
            app:destination="@id/adminAddFragment" />
        <action
            android:id="@+id/action_adminFragment2_to_adminListFragment"
            app:destination="@id/adminListFragment" />
        <action
            android:id="@+id/action_adminFragment2_to_signFragment2"
            app:destination="@id/signFragment2" />
    </fragment>
    <fragment
        android:id="@+id/adminListFragment"
        android:name="com.example.uzbekistan.fragmnets.AdminListFragment"
        android:label="fragment_admin_list"
        tools:layout="@layout/fragment_admin_list" >
        <action
            android:id="@+id/action_adminListFragment_to_adminFragment2"
            app:destination="@id/adminFragment2" />
    </fragment>
    <fragment
        android:id="@+id/adminAddFragment"
        android:name="com.example.uzbekistan.fragmnets.AdminAddFragment"
        android:label="fragment_admin_add"
        tools:layout="@layout/fragment_admin_add" >
        <action
            android:id="@+id/action_adminAddFragment_to_adminFragment2"
            app:destination="@id/adminFragment2" />
    </fragment>
    <fragment
        android:id="@+id/signFragment2"
        android:name="com.example.uzbekistan.fragmnets.SignFragment"
        android:label="fragment_sign"
        tools:layout="@layout/fragment_sign" />
</navigation>